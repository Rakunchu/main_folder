<script>
      var slider = document.getElementById("elephant");
      var output = document.getElementById("jet");
      output.innerHTML = slider.value;
    
      slider.oninput = function(){
          output.innerHTML = this.value;
      }
    </script>
<script>
document.getElementById("baseball").style.visibility ="hidden";

function ttt{
	const baseball = document.getElementById("baseball");

	if(baseball.style.visibility=="visible"){
		baseball.style.visibility ="hidden";
	}else{
		baseball.style.visibility ="visible";
	}
}
</script>


